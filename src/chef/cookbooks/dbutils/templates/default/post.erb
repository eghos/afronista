<% if @users == "true" %>

LOCK TABLES `admin_role` WRITE , `admin_user` WRITE;

<% @user_passwords.each do |user_password|  %>
SET @SALT = "rp";
SET @PASS = CONCAT(MD5(CONCAT( @SALT , "<%= user_password[1] %>") ), CONCAT(":", @SALT ));
SELECT @EXTRA := MAX(extra) FROM admin_user WHERE extra IS NOT NULL;

UPDATE `admin_user` SET password = @PASS WHERE username = '<%= user_password[0] %>';
UPDATE `admin_user` SET extra = @EXTRA WHERE username = '<%= user_password[0] %>';
UPDATE `admin_user` SET created = NOW() WHERE username = '<%= user_password[0] %>';
UPDATE `admin_user` SET rp_token_created_at = NOW() WHERE username = '<%= user_password[0] %>';
<% end %>

<% @admins.each do |admin| %>
UPDATE `admin_role` SET parent_id = 1 WHERE user_id = ( SELECT user_id FROM admin_user where username = '<%= admin %>' );
<% end %>

UNLOCK TABLES;
<% end %>

